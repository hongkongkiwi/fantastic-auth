name: vault_sdk
repository: https://github.com/yourorg/vault/tree/main/vault-sdk-flutter
issue_tracker: https://github.com/yourorg/vault/issues

version: 0.1.0

environment:
  sdk: '>=3.0.0 <4.0.0'
  flutter: ">=3.10.0"

dependencies:
  flutter:
    sdk: flutter
  http: ^1.1.0
  flutter_secure_storage: ^9.0.0
  local_auth: ^2.1.8
  url_launcher: ^6.1.14
  crypto: ^3.0.3
  json_annotation: ^4.8.1
  meta: ^1.9.1

flutter:
  plugin:
    platforms:
      android:
        package: dev.vault.sdk
        pluginClass: VaultSdkPlugin
      ios:
        pluginClass: VaultSdkPlugin
