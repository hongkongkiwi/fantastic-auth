# Vault Web UI environment

# Internal API (server-side)
INTERNAL_API_BASE_URL=http://localhost:3000/api/v1/internal
INTERNAL_API_KEY=
INTERNAL_UI_TOKEN=
INTERNAL_UI_PASSWORD=
INTERNAL_UI_AUDIT_STORAGE=file
LOG_LEVEL=info

# Client-side defaults (Vite)
VITE_INTERNAL_API_BASE_URL=
VITE_INTERNAL_UI_TOKEN=

# Sentry (client runtime)
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1
VITE_SENTRY_TRACES_SAMPLE_RATE_HIGH=
VITE_SENTRY_TRACES_SAMPLE_RATE_HIGH_ROUTES=/login,/settings
VITE_SENTRY_TRACES_SAMPLE_RATE_LOW=
VITE_SENTRY_TRACES_SAMPLE_RATE_LOW_ROUTES=/health

# Sentry source map upload (build time)
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_RELEASE=

# CAPTCHA Providers - Set to 'true' to enable each provider in the UI
VITE_CAPTCHA_RECAPTCHA_V2_ENABLED=false
VITE_CAPTCHA_RECAPTCHA_V3_ENABLED=false
VITE_CAPTCHA_HCAPTCHA_ENABLED=false
VITE_CAPTCHA_TURNSTILE_ENABLED=false

# OAuth/Social Login Providers - Set to 'true' to enable each provider
VITE_OAUTH_GOOGLE_ENABLED=false
VITE_OAUTH_GITHUB_ENABLED=false
VITE_OAUTH_MICROSOFT_ENABLED=false
VITE_OAUTH_APPLE_ENABLED=false

# Email Providers - Set to 'true' to enable each provider
VITE_EMAIL_SMTP_ENABLED=false
VITE_EMAIL_SENDGRID_ENABLED=false
VITE_EMAIL_MAILGUN_ENABLED=false
VITE_EMAIL_AWS_SES_ENABLED=false
VITE_EMAIL_POSTMARK_ENABLED=false
VITE_EMAIL_RESEND_ENABLED=false

# SMS Providers - Set to 'true' to enable each provider
VITE_SMS_TWILIO_ENABLED=false
VITE_SMS_MESSAGE_BIRD_ENABLED=false
VITE_SMS_VONAGE_ENABLED=false

# Storage Providers - Set to 'true' to enable each provider
VITE_STORAGE_S3_ENABLED=false
VITE_STORAGE_R2_ENABLED=false
VITE_STORAGE_AZURE_BLOB_ENABLED=false

# Payment Providers - Set to 'true' to enable each provider
VITE_PAYMENT_STRIPE_ENABLED=false
VITE_PAYMENT_PADDLE_ENABLED=false

# Analytics - Set to 'true' to enable
VITE_ANALYTICS_POSTHOG_ENABLED=false
VITE_ANALYTICS_PLAUSIBLE_ENABLED=false

# Security Services - Set to 'true' to enable
VITE_SECURITY_HIBP_ENABLED=false
VITE_SECURITY_MAXMIND_ENABLED=false
