#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andy/Development/hongkongkiwi/fantastic-auth/packages/apps/web-platform/node_modules/.pnpm/h3@2.0.1-rc.14/node_modules/h3/bin/node_modules:/Users/andy/Development/hongkongkiwi/fantastic-auth/packages/apps/web-platform/node_modules/.pnpm/h3@2.0.1-rc.14/node_modules/h3/node_modules:/Users/andy/Development/hongkongkiwi/fantastic-auth/packages/apps/web-platform/node_modules/.pnpm/h3@2.0.1-rc.14/node_modules:/Users/andy/Development/hongkongkiwi/fantastic-auth/packages/apps/web-platform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/andy/Development/hongkongkiwi/fantastic-auth/packages/apps/web-platform/node_modules/.pnpm/h3@2.0.1-rc.14/node_modules/h3/bin/node_modules:/Users/andy/Development/hongkongkiwi/fantastic-auth/packages/apps/web-platform/node_modules/.pnpm/h3@2.0.1-rc.14/node_modules/h3/node_modules:/Users/andy/Development/hongkongkiwi/fantastic-auth/packages/apps/web-platform/node_modules/.pnpm/h3@2.0.1-rc.14/node_modules:/Users/andy/Development/hongkongkiwi/fantastic-auth/packages/apps/web-platform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../h3-v2/bin/h3.mjs" "$@"
else
  exec node  "$basedir/../h3-v2/bin/h3.mjs" "$@"
fi
