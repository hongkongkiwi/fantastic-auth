{
  "name": "@fantasticauth/svelte",
  "version": "0.1.0",
  "description": "Svelte SDK for Vault - Secure user management with Svelte 4/5 support",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "build": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.svelte",
    "format": "prettier --write .",
    "prepare": "npm run build"
  },
  "keywords": [
    "auth",
    "authentication",
    "user-management",
    "svelte",
    "sveltekit",
    "vault",
    "security",
    "oauth",
    "mfa",
    "runes",
    "stores"
  ],
  "author": "Vault Team",
  "license": "MIT",
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0 || ^4.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.1.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@fantasticauth/sdk": "file:../../../generated/typescript"
  }
}
