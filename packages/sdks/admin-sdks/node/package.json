{
  "name": "@fantasticauth/auth",
  "version": "1.0.0",
  "description": "Official Node.js SDK for Vault authentication and user management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./middleware/express": {
      "import": "./dist/middleware/express.js",
      "require": "./dist/middleware/express.cjs",
      "types": "./dist/middleware/express.d.ts"
    },
    "./middleware/fastify": {
      "import": "./dist/middleware/fastify.js",
      "require": "./dist/middleware/fastify.cjs",
      "types": "./dist/middleware/fastify.d.ts"
    },
    "./middleware/koa": {
      "import": "./dist/middleware/koa.js",
      "require": "./dist/middleware/koa.cjs",
      "types": "./dist/middleware/koa.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vault",
    "auth",
    "authentication",
    "jwt",
    "oauth",
    "sso",
    "sdk"
  ],
  "author": "Vault Team <support@vault.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vault/auth-sdk-node.git"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "axios-retry": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/koa": "^2.14.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "express": "^4.18.0",
    "fastify": "^4.24.0",
    "jest": "^29.7.0",
    "koa": "^2.14.0",
    "nock": "^13.4.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0",
    "fastify": "^4.0.0 || ^5.0.0",
    "koa": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "koa": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
